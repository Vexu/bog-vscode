{
    "name": "bog",
    "displayName": "Bog",
    "description": "Language support for Bog",
    "version": "0.1.0",
    "publisher": "Vexu",
    "repository": {
        "type": "git",
        "url": "https://github.com/Vexu/bog-vscode"
    },
    "bugs": {
        "url": "https://github.com/Vexu/bog-vscode/issues"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "bog",
                "extensions": [
                    ".bog",
                    ".bbog"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bog",
                "scopeName": "source.bog",
                "path": "./syntaxes/bog.tmLanguage.json"
            },
            {
                "scopeName": "todo-comment.injection",
                "injectTo": ["source.bog"],
                "path": "./syntaxes/comment-injection.json"
            }
        ]
    }
}